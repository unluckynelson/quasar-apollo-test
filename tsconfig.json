{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "@viatu/*": ["src/*"],
      "vue-inline-svg": ["shims/vue-inline-svg"]
    },
    "typeRoots": [
      "../shims",
      "../node_modules/@types",
      "../node_modules/@quasar/app/types",
    ],
    "noImplicitAny": false,
  },
  "exclude": ["/dist", ".quasar", "/node_modules", "/shims"],
  "ignorePatterns": [".eslintrc.js", "babel.config.js"],
}
